{
  "npmClient": "yarn",
  "private": true,
  "author": "Denys Vuika <denys.vuika@gmail.com>",
  "scripts": {
    "dev": "lerna run start --parallel --ignore docs",
    "dev:yalc": "lerna run dev:yalc --parallel",
    "clean": "lerna run clean --stream",
    "build": "lerna run build --stream --ignore 'example-*'",
    "build:all": "lerna run build --stream",
    "deploy": "cross-env ENV=production ./scripts/deploy.sh",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bump": "lerna version --no-push --no-git-tag-version --force-publish",
    "publish": "run-s build lint test && lerna publish from-git --no-private --no-push",
    "prepare": "husky"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "13.4.0",
    "@types/jest": "^29.5.12",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^9.0.11",
    "jest": "29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "lerna": "7.3.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rollup": "^4.12.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ]
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenysVuika/handicraft.git"
  },
  "packageManager": "yarn@3.6.3"
}
